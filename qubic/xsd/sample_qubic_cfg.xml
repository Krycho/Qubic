<?xml version="1.0" encoding="UTF-8"?>
<q:qubic xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:q="http://www.example.org/Qubic"
       xsi:schemaLocation="http://www.example.org/Qubic qubic.xsd">

    <q:credentials>
        <q:username>test</q:username>
        <q:password>test</q:password>
    </q:credentials>
    <q:connection name="qubic_Test">
        <q:schema name="qubic"/>
        <q:driver name="mysql"/>
        <q:host address="localhost" port="3306"/>
    </q:connection>

    <q:querying>
        <q:option name="autoCommit">d</q:option>
        <q:option name="useUnicode">true</q:option>
    </q:querying>

    <q:qubes path='./this/is/path/to/model/'>
        <q:qube type="class" source="Bike.h" path='./subPath/'/>
        <q:qube type="file" source="car.qube"/>
    </q:qubes>
</q:qubic>